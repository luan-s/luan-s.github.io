doctype
html
	head
		title Luan Souza

		meta(charset="utf-8")
		meta(content="width=device-width, initial-scale=1.0" name="viewport")
		link(rel="stylesheet" type="text/css" href="static/css/animate.css")
		link(rel="stylesheet" type="text/css" href="static/css/style.css")
		link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css")

	body
		.content
			nav.transparent
				.nav-wrapper
					a.brand-logo(href="#!") Luan Souza
					ul.right.hide-on-med-and-down
						li
							a(href="sass.html") Social
						

			main.row
				.fundo.col-md-13
					img(src="img/fundo2.jpg" )
				.header.center.col-md-12.transparent
					a.btn-floating.col.md.12.photo.animated.bounceInLeft
						i.material-icons

		
		section.center.col-md-13.skills.row
			h1.title.col-md-13.center#trab Trabalhos
			.jobs-box.col-md-6.col-xs-12.center.col-lg-3.border.wow.flipInX
				img(src="img/lbook.png").photo-card
			.jobs-box.col-md-6.col-xs-12.col-lg-3.center.wow.flipInX
				img(src="img/table.png").photo-card
			.jobs-box.col-md-6.col-xs-12.col-lg-3.center.border.wow.flipInX
				img(src="img/lphotos.png").photo-card
			.jobs-box.col-md-6.col-xs-12.col-lg-3.center.wow.flipInX
				img(src="img/loading.png").photo-card
			

		section.col-md-13.jobs.row
			img(src="img/fundo.jpg")
		section.col-md-13.social.row
			h1.title.col-md-13.center Social
			span.social-box.col-md-6.col-xs-12.center.border
				i.fa.fa-facebook-square.icons.wow.flipInX
			span.social-box.col-md-6.col-xs-12.center
				i.fa.fa-instagram.icons.wow.flipInX
			span.social-box.col-md-6.col-xs-12.center.border
				i.fa.fa-google-plus-square.icons.wow.flipInX
			span.social-box.col-md-6.col-xs-12.center
				i.fa.fa-whatsapp.icons.wow.flipInX

		

		script(type="text/javascript" src="static/js/script.js")
		script(type="text/javascript" src="static/js/wow.min.js")
		script.
			wow = new WOW(
			{
				boxClass:     'wow',      // default
				animateClass: 'animated', // default
				offset:       100,          // default
				mobile:       true,       // default
				live:         true        // default
			}
			)
			wow.init();

		



